{
    "extends": "wikimedia",
    "env": {
        "node": true,
        "es6": true
    },
    "rules": {
        "array-bracket-spacing": "off",
        "block-spacing": "off",
        "comma-spacing": "off",
        "computed-property-spacing": "off",
        "indent": "off",
        "key-spacing": "off",
        "keyword-spacing": "off",
        "new-cap": "off",
        "no-console": "off",
        "no-mixed-spaces-and-tabs": "off",
        "no-multi-spaces": "off",
        "no-trailing-spaces": "off",
        "no-underscore-dangle": "off",
        "no-unused-vars": "off",
        "no-use-before-define": "off",
        "no-useless-escape": "off",
        "object-curly-spacing": "off",
        "one-var": "off",
        "space-before-blocks": "off",
        "space-before-function-paren": "off",
        "space-in-parens": "off",
        "space-infix-ops": "off",
        "spaced-comment": "off",
        "vars-on-top": "off"
    }
}
